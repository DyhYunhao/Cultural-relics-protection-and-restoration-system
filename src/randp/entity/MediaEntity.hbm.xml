<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="randp.entity.MediaEntity" table="media" schema="randp">
        <id name="mid" column="mid"/>
        <property name="crname" column="crname"/>
        <property name="mtype" column="mtype"/>
        <property name="video" column="video"/>
        <property name="mcomment" column="mcomment"/>
        <property name="vname" column="vname"/>
    </class>
</hibernate-mapping>