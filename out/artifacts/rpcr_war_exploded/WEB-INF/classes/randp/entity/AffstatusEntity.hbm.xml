<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="randp.entity.AffstatusEntity" table="affstatus" schema="randp">
        <id name="asid" column="asid"/>
        <property name="assize" column="assize"/>
        <property name="asweight" column="asweight"/>
        <property name="aspicture" column="aspicture"/>
        <property name="assave" column="assave"/>
        <property name="asvedio" column="asvedio"/>
        <property name="astime" column="astime"/>
        <property name="asname" column="asname"/>
    </class>
</hibernate-mapping>