<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="randp.entity.UserdetailEntity" table="userdetail" schema="randp">
        <id name="udid" column="udid"/>
        <property name="uid" column="uid"/>
        <property name="phone" column="phone"/>
        <property name="post" column="post"/>
        <property name="typeid" column="typeid"/>
        <property name="unit" column="unit"/>
        <property name="address" column="address"/>
        <property name="idcard" column="idcard"/>
    </class>
</hibernate-mapping>