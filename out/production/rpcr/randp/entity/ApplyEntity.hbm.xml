<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="randp.entity.ApplyEntity" table="apply" schema="randp">
        <id name="aid" column="aid"/>
        <property name="crname" column="crname"/>
        <property name="exname" column="exname"/>
        <property name="extime" column="extime"/>
        <property name="rtname" column="rtname"/>
        <property name="rttime" column="rttime"/>
        <property name="rsname" column="rsname"/>
        <property name="apunit" column="apunit"/>
        <property name="aptime" column="aptime"/>
        <property name="apnum" column="apnum"/>
    </class>
</hibernate-mapping>